      </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Add Product Section -->\r\n    <div class=\"form-section\">\r\n      <div class=\"add-product-form\">\r\n        <h3>Add New Product</h3>\r\n        <form @submit.prevent=\"addProduct\">\r\n          <div class=\"input-group\">\r\n            <label for=\"productName\">\r\n              <input type=\"text\" v-model=\"newProductName\" placeholder=\"Product Name\" />\r\n            </label>\r\n          </div>\r\n          <div class=\"input-group\">\r\n            <label for=\"brandName\">\r\n              <input type=\"text\" v-model=\"newBrandName\" placeholder=\"Brand Name\" />\r\n            </label>\r\n          </div>\r\n          <button type=\"submit\" class=\"add-product-button\">Add Product</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ProductList',\r\n  data() {\r\n    return {\r\n      products: [], // Initialize products array\r\n      newProductName: '',\r\n      newBrandName: ''\r\n    };\r\n  },\r\n  methods: {\r\n    // Fetch products from the backend\r\n    async fetchProducts() {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        const response = await axios.get('https://iinfluencer.net/api/product', {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        this.products = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching products:', error);\r\n      }\r\n    },\r\n    // Add a new product to the backend\r\n    async addProduct() {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        const response = await axios.post('https://iinfluencer.net/api/product/store', {\r\n          name: this.newProductName,\r\n          brand_id: 1 // Replace with the appropriate brand ID or provide a mechanism to select it\r\n        }, {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        this.products.push(response.data); // Assuming the API returns the created product\r\n        this.newProductName = '';\r\n        this.newBrandName = '';\r\n      } catch (error) {\r\n        console.error('Error adding product:', error);\r\n      }\r\n    },\r\n    // Delete a product from the backend\r\n    async deleteProduct(productId) {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        await axios.delete(`https://iinfluencer.net/api/product/delete/${productId}`, {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        this.products = this.products.filter(product => product.id !== productId);\r\n      } catch (error) {\r\n        console.error('Error deleting product:', error);\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchProducts(); // Fetch products when the component is mounted\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Your existing styles, including styling for input groups, buttons, and the table */\r\n</style>\r\n\r\n\r\n  \r\n  <style scoped> \r\n  .content-container {\r\n  display: grid;\r\n  margin-top: -120px; \r\n  grid-template-columns: 2fr 1fr; /* Adjust the width ratio as needed */\r\n  gap: 20px; /* Space between the table and form */\r\n  padding: 20px;\r\n}\r\n\r\n.form-section {\r\n  /* Style specific to the form section */\r\n  background-color: #f9f9f9;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n \r\n.input-group {\r\n  margin-bottom: 15px;\r\n  position: relative;\r\n}\r\n\r\n.input-group label {\r\n  display: block;\r\n  position: relative;\r\n  width: 100%;\r\n}\r\n\r\n.input-group input {\r\n  width: 100%;\r\n  padding: 15px;\r\n  padding-left: 20px; /* Adjust padding since there is no icon */\r\n  border-radius: 30px;\r\n  border: 2px solid #C6CEDC;\r\n  font-size: 16px;\r\n  outline: none;\r\n  transition: border-color 0.3s ease;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.input-group input:focus {\r\n  border-color: #86ADC3;\r\n}\r\n\r\n/* Styling for the submit button similar to the login button */\r\n.add-product-button {\r\n  width: 100%;\r\n  padding: 15px;\r\n  background: #E8C6CC;\r\n  border-radius: 30px;\r\n  border: none;\r\n  font-family: 'Urbanist', sans-serif;\r\n  font-size: 16px;\r\n  color: #FFFFFF;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.add-product-button:hover {\r\n  background-color: #D6A6B4; /* Darken on hover if desired */\r\n}\r\n\r\n\r\n\r\n.delete-button {\r\n  background-color: transparent;\r\n  border: none;\r\n  color: #ff4c4c; /* Red color */\r\n  cursor: pointer;\r\n  font-size: 18px;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.delete-button:hover {\r\n  color: #ff0000; /* Darker red on hover */\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n  /* Add a border and rounding to the table */\r\n.table-container {\r\n  \r\n  border: 1px solid #ddd;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  overflow-x: auto;\r\n  /* Ensures the border radius is applied to the whole table */\r\n}\r\n\r\n/* Style for table rows */\r\n.table-container table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.table-container th, .table-container td {\r\n  padding: 12px 15px;\r\n  text-align: left;\r\n}\r\n.table-container thead th {\r\n  position: sticky;\r\n  top: 0;\r\n  background-color: #f2f2f2;\r\n  z-index: 1; /* Ensure the header is above the table rows */\r\n  border-bottom: 2px solid #ddd; /* Add a bottom border for the header */\r\n}\r\n\r\n/* Add borders to rows */\r\n.table-container tr {\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n/* Style for the header row */\r\n.table-container th {\r\n  background-color: #f2f2f2;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Round the top corners of the header */\r\n.table-container th:first-child {\r\n  border-top-left-radius: 10px;\r\n}\r\n\r\n.table-container th:last-child {\r\n  border-top-right-radius: 10px;\r\n}\r\n\r\n/* Optionally round the bottom corners of the last row */\r\n.table-container tr:last-child td:first-child {\r\n  border-bottom-left-radius: 10px;\r\n}\r\n\r\n.table-container tr:last-child td:last-child {\r\n  border-bottom-right-radius: 10px;\r\n}\r\n\r\n/* Add hover effect for rows */\r\n.table-container tr:hover {\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n  .product-table {\r\n    width: 100%;\r\n    margin: 20px auto;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  th, td {\r\n    padding: 15px;\r\n    text-align: left;\r\n  }\r\n  \r\n  th {\r\n    background-color: #C6CEDC; /* Table header color */\r\n    color: black;\r\n    font-family: 'Urbanist', sans-serif;\r\n    font-weight: 600;\r\n  }\r\n  \r\n  td {\r\n    background-color: #FFFFFF; /* Row color */\r\n    color: black;\r\n    font-family: 'Urbanist', sans-serif;\r\n    font-weight: 400;\r\n  }\r\n  \r\n  tr:nth-child(even) td {\r\n    background-color:  #FFFFFF; /* Alternate row color */\r\n  }\r\n  \r\n  .product-info {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  .product-image {\r\n    width: 50px;\r\n    height: 50px;\r\n    border-radius: 8px;\r\n    margin-right: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;;;;EACO,SAAM;AAAmB;;EAEvB,SAAM;AAAe;;EACnB,SAAM;AAAiB;;EACrB,SAAM;AAAe;;sBAEtBA,mBAAA,CAMQ,gB,aALNA,mBAAA,CAIK,a,aAHHA,mBAAA,CAAgB,YAAZ,SAAO,G,aACXA,mBAAA,CAAc,YAAV,OAAK,G,aACTA,mBAAA,CAAgB,YAAZ,SAAO,E;;;EAKP,SAAM;AAAc;iBAhBxC;;EAiCS,SAAM;AAAc;;EAClB,SAAM;AAAkB;;sBAC3BA,mBAAA,CAAwB,YAApB,iBAAe;AAAA;;EAEZ,SAAM;AAAa;;EACf,OAAI;AAAa;;EAIrB,SAAM;AAAa;;EACf,OAAI;AAAW;;sBAIxBA,mBAAA,CAAqE;IAA7DC,IAAI,EAAC,QAAQ;IAAC,SAAM;KAAqB,aAAW;AAAA;;uBA9CpEC,mBAAA,CAkDM,OAlDNC,UAkDM,GAjDJC,mBAAA,mBAAsB,EACtBJ,mBAAA,CA2BM,OA3BNK,UA2BM,GA1BJL,mBAAA,CAyBM