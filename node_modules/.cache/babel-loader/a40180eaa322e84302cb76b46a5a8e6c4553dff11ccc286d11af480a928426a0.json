;\r\n}\r\n\r\n \r\n.input-group {\r\n  margin-bottom: 15px;\r\n  position: relative;\r\n}\r\n\r\n.input-group label {\r\n  display: block;\r\n  position: relative;\r\n  width: 100%;\r\n}\r\n\r\n.input-group input {\r\n  width: 100%;\r\n  padding: 15px;\r\n  padding-left: 20px; /* Adjust padding since there is no icon */\r\n  border-radius: 30px;\r\n  border: 2px solid #C6CEDC;\r\n  font-size: 16px;\r\n  outline: none;\r\n  transition: border-color 0.3s ease;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.input-group input:focus {\r\n  border-color: #86ADC3;\r\n}\r\n\r\n/* Styling for the submit button similar to the login button */\r\n.add-product-button {\r\n  width: 100%;\r\n  padding: 15px;\r\n  background: #E8C6CC;\r\n  border-radius: 30px;\r\n  border: none;\r\n  font-family: 'Urbanist', sans-serif;\r\n  font-size: 16px;\r\n  color: #FFFFFF;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.add-product-button:hover {\r\n  background-color: #D6A6B4; /* Darken on hover if desired */\r\n}\r\n\r\n\r\n\r\n.delete-button {\r\n  background-color: transparent;\r\n  border: none;\r\n  color: #ff4c4c; /* Red color */\r\n  cursor: pointer;\r\n  font-size: 18px;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.delete-button:hover {\r\n  color: #ff0000; /* Darker red on hover */\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n  /* Add a border and rounding to the table */\r\n.table-container {\r\n  \r\n  border: 1px solid #ddd;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  overflow-x: auto;\r\n  /* Ensures the border radius is applied to the whole table */\r\n}\r\n\r\n/* Style for table rows */\r\n.table-container table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.table-container th, .table-container td {\r\n  padding: 12px 15px;\r\n  text-align: left;\r\n}\r\n.table-container thead th {\r\n  position: sticky;\r\n  top: 0;\r\n  background-color: #f2f2f2;\r\n  z-index: 1; /* Ensure the header is above the table rows */\r\n  border-bottom: 2px solid #ddd; /* Add a bottom border for the header */\r\n}\r\n\r\n/* Add borders to rows */\r\n.table-container tr {\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n/* Style for the header row */\r\n.table-container th {\r\n  background-color: #f2f2f2;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Round the top corners of the header */\r\n.table-container th:first-child {\r\n  border-top-left-radius: 10px;\r\n}\r\n\r\n.table-container th:last-child {\r\n  border-top-right-radius: 10px;\r\n}\r\n\r\n/* Optionally round the bottom corners of the last row */\r\n.table-container tr:last-child td:first-child {\r\n  border-bottom-left-radius: 10px;\r\n}\r\n\r\n.table-container tr:last-child td:last-child {\r\n  border-bottom-right-radius: 10px;\r\n}\r\n\r\n/* Add hover effect for rows */\r\n.table-container tr:hover {\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n  .product-table {\r\n    width: 100%;\r\n    margin: 20px auto;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  th, td {\r\n    padding: 15px;\r\n    text-align: left;\r\n  }\r\n  \r\n  th {\r\n    background-color: #C6CEDC; /* Table header color */\r\n    color: black;\r\n    font-family: 'Urbanist', sans-serif;\r\n    font-weight: 600;\r\n  }\r\n  \r\n  td {\r\n    background-color: #FFFFFF; /* Row color */\r\n    color: black;\r\n    font-family: 'Urbanist', sans-serif;\r\n    font-weight: 400;\r\n  }\r\n  \r\n  tr:nth-child(even) td {\r\n    background-color:  #FFFFFF; /* Alternate row color */\r\n  }\r\n  \r\n  .product-info {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  .product-image {\r\n    width: 50px;\r\n    height: 50px;\r\n    border-radius: 8px;\r\n    margin-right: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;;;;EACO,SAAM;AAAmB;;EAEvB,SAAM;AAAe;;EACnB,SAAM;AAAiB;;EACrB,SAAM;AAAe;;sBAEtBA,mBAAA,CAMQ,gB,aALNA,mBAAA,CAIK,a,aAHHA,mBAAA,CAAgB,YAAZ,SAAO,G,aACXA,mBAAA,CAAc,YAAV,OAAK