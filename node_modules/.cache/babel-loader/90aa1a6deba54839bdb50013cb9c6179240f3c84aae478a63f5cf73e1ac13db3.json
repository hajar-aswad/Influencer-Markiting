e\">\r\n          <table>\r\n            <thead>\r\n              <tr>\r\n                <th>PRODUCT</th>\r\n                <th>BRAND</th>\r\n                <th>Actions</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(product, index) in products\" :key=\"index\">\r\n                <td class=\"product-info\">\r\n                  <span>{{ product.name }}</span>\r\n                </td>\r\n                <td>{{ product.brand }}</td> \r\n                <td>\r\n                  <button class=\"delete-button\" @click=\"deleteProduct(index)\">\r\n                    üóëÔ∏è\r\n                  </button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Add Product Section -->\r\n    <div class=\"form-section\">\r\n    <div class=\"add-product-form\">\r\n      <h3>Add New Product</h3>\r\n      <form @submit.prevent=\"addProduct\">\r\n        <div class=\"input-group\">\r\n          <label for=\"productName\">\r\n            <input type=\"text\" v-model=\"newProductName\" placeholder=\"Product Name\" />\r\n          </label>\r\n        </div>\r\n        <div class=\"input-group\">\r\n          <label for=\"brandName\">\r\n            <input type=\"text\" v-model=\"newBrandName\" placeholder=\"Brand Name\" />\r\n          </label>\r\n        </div>\r\n        <button type=\"submit\" class=\"add-product-button\">Add Product</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n\r\n  export default {\r\n  name: 'ProductList',\r\n  props: {\r\n    products: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      products: []\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchProducts() {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        c