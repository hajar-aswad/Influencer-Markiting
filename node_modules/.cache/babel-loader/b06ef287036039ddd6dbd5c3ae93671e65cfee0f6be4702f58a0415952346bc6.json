  <tr>\r\n                <th>PRODUCT</th>\r\n                <th>BRAND</th>\r\n                <th>Actions</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(product, index) in products\" :key=\"index\">\r\n                <td class=\"product-info\">\r\n                  <span>{{ product.name }}</span>\r\n                </td>\r\n                <td>{{ product.brand }}</td> \r\n                <td>\r\n                  <button class=\"delete-button\" @click=\"deleteProduct(product.id)\">\r\n                    üóëÔ∏è\r\n                  </button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Add Product Section -->\r\n    <div class=\"form-section\">\r\n      <div class=\"add-product-form\">\r\n        <h3>Add New Product</h3>\r\n        <form @submit.prevent=\"addProduct\">\r\n          <div class=\"input-group\">\r\n            <label for=\"productName\">\r\n              <input type=\"text\" v-model=\"newProductName\" placeholder=\"Product Name\" />\r\n            </label>\r\n          </div>\r\n          <div class=\"input-group\">\r\n            <label for=\"brandSelect\"> <!-- Update the label for the brand dropdown -->\r\n              <select v-model=\"selectedBrandId\">\r\n                <option disabled value=\"\">Please select a brand</option>\r\n                <option v-for=\"brand in brands\" :key=\"brand.company_id\" :value=\"brand.company_id\">\r\n                  {{ brand.name }}\r\n                </option>\r\n              </select>\r\n            </label>\r\n          </div>\r\n          <button type=\"submit\" class=\"add-product-button\">Add Product</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ProductList',\r\n  data() {\r\n    return {\r\n      products: [], // Initialize products array\r\n      brands: [], // Initialize brands array\r\n      newProductName: '',\r\n      selectedBrandId: '', // Track the selected brand ID\r\n    };\r\n  },\r\n  methods: {\r\n    // Fetch products from the backend\r\n    async fetchProducts() {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        const response = await axios.get('https://iinfluencer.net/api/product', {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        this.products = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching products:', error);\r\n      }\r\n    },\r\n    // Fetch brands from the backend\r\n    async fetchBrands() {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        const response = await axios.get('https://iinfluencer.net/api/brand', {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        this.brands = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching brands:', error);\r\n      }\r\n    },\r\n    // Add a new product to the backend\r\n    async addProduct() {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        const response = await axios.post('https://iinfluencer.net/api/product/store', {\r\n          name: this.newProductName,\r\n          brand_id: this.selectedBrandId // Use the selected brand ID\r\n        }, {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        this.products.push(response.data); // Assuming the API returns the created product\r\n        this.newProductName = '';\r\n        this.selectedBrandId = ''; // Reset the selected brand ID\r\n      } catch (error) {\r\n        console.error('Error adding product:', error);\r\n      }\r\n    },\r\n    // Delete a product from the backend\r\n    async deleteProduct(productId) {\r\n      const token = localStorage.getItem('access_token');\r\n      try {\r\n        await axios.delete(`https://iinfluencer.net/api/product/delete/${productId}`, {\r\n          headers: {\r\n            Authorization: `Bearer ${token}`\r\n          }\r\n        });\r\n        this.products = this.products.filter(product => product.id !== productId);\r\n      } catch (error) {\r\n        console.error('Error deleting product:', error);\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchProducts(); // Fetch products when the component is mounted\r\n    this.fetchBrands(); // Fetch brands when the component is mounted\r\n  }\r\n};\r\n</script> \r\n\r\n\r\n\r\n  \r\n  <style scoped> \r\n  .content-container {\r\n  display: grid;\r\n  margin-top: -120px; \r\n  grid-template-columns: 2fr 1fr; /* Adjust the width ratio as needed */\r\n  gap: 20px; /* Space between the table and form */\r\n  padding: 20px;\r\n}\r\n\r\n.form-section {\r\n  /* Style specific to the form section */\r\n  background-color: #f9f9f9;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n \r\n.input-group {\r\n  margin-bottom: 15px;\r\n  position: relative;\r\n}\r\n\r\n\r\n\r\n.input-group label {\r\n  display: block;\r\n  position: relative;\r\n  width: 100%;\r\n}\r\n.input-group select {\r\n  width: 100%;\r\n  padding: 15px;\r\n  padding-left: 20px;\r\n  border-radius: 30px;\r\n  border: 2px solid #C6CEDC;\r\n  font-size: 16px;\r\n  outline: none;\r\n  transition: border-color 0.3s ease;\r\n  box-sizing: border-box;\r\n  background-color: white;\r\n  appearance: none !important; /* Remove default dropdown arrow */\r\n  -webkit-appearance: none !important;\r\n  -moz-appearance: none !important;\r\n  background-image: url('data:image/svg+xml;charset=US-ASCII,<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 20 20\"><polygon points=\"0,0 20,0 10,10\" fill=\"#C6CEDC\"/></svg>');\r\n  background-repeat: no-repeat;\r\n  background-position: right 10px top 50%;\r\n  background-size: 15px 15px;\r\n  color: #6c757d; /* Match text color with input fields */\r\n}\r\n\r\n.input-group select:focus {\r\n  border-color: #86ADC3;\r\n}\r\n\r\n.input-group select::-ms-expand {\r\n  display: none; /* Remove the dropdown arrow in IE */\r\n}\r\n.input-group input:focus {\r\n  border-color: #7ba0b6;\r\n}\r\n\r\n\r\n.input-group input {\r\n  width: 100%;\r\n  padding: 15px;\r\n  padding-left: 20px; /* Adjust padding since there is no icon */\r\n  border-radius: 30px;\r\n  border: 2px solid #C6CEDC;\r\n  font-size: 16px;\r\n  outline: none;\r\n  transition: border-color 0.3s ease;\r\n  box-sizing: border-box;\r\n  background-color: white; /* Ensure the background is white */\r\n  appearance: none !important; /* Ensure there is no arrow on input fields */\r\n  -webkit-appearance: none !important; /* Override default styles */\r\n  -moz-appearance: none !important;\r\n}\r\n\r\n\r\n.input-group input:focus {\r\n  border-color: #86ADC3;\r\n}  \r\n/*  to delet this*/      \r\n.input-group select + .after {\r\n  display: block;\r\n}\r\n\r\n.input-group input + .after {\r\n  display: none; /* Ensure no arrow is shown for input fields */\r\n}\r\n\r\n.input-group::after {\r\n  content: '';\r\n  position: absolute;\r\n  right: 15px;\r\n  top: 50%;\r\n  margin-top: -2px;\r\n  border-width: 5px;\r\n  border-style: solid;\r\n  border-color: #C6CEDC transparent transparent transparent;\r\n  pointer-events: none;\r\n}\r\n\r\n/* Styling for the submit button similar to the login button */\r\n.add-product-button {\r\n  width: 100%;\r\n  padding: 15px;\r\n  background: #E8C6CC;\r\n  border-radius: 30px;\r\n  border: none;\r\n  font-family: 'Urbanist', sans-serif;\r\n  font-size: 16px;\r\n  color: #FFFFFF;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.add-product-button:hover {\r\n  background-color: #D6A6B4; /* Darken on hover if desired */\r\n}\r\n\r\n\r\n\r\n.delete-button {\r\n  background-color: transparent;\r\n  border: none;\r\n  color: #ff4c4c; /* Red color */\r\n  cursor: pointer;\r\n  font-size: 18px;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.delete-button:hover {\r\n  color: #ff0000; /* Darker red on hover */\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n  /* Add a border and rounding to the table */\r\n.table-container {\r\n  \r\n  border: 1px solid #ddd;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  overflow-x: auto;\r\n  /* Ensures the border radius is applied to the whole table */\r\n}\r\n\r\n/* Style for table rows */\r\n.table-container table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.table-container th, .table-container td {\r\n  padding: 12px 15px;\r\n  text-align: left;\r\n}\r\n.table-container thead th {\r\n  position: sticky;\r\n  top: 0;\r\n  background-color: #f2f2f2;\r\n  z-index: 1; /* Ensure the header is above the table rows */\r\n  border-bottom: 2px solid #ddd; /* Add a bottom border for the header */\r\n}\r\n\r\n/* Add borders to rows */\r\n.table-container tr {\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n/* Style for the header row */\r\n.table-container th {\r\n  background-color: #f2f2f2;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Round the top corners of the header */\r\n.table-container th:first-child {\r\n  border-top-left-radius: 10px;\r\n}\r\n\r\n.table-container th:last-child {\r\n  border-top-right-radius: 10px;\r\n}\r\n\r\n/* Optionally round the bottom corners of the last row */\r\n.table-container tr:last-child td:first-child {\r\n  border-bottom-left-radius: 10px;\r\n}\r\n\r\n.table-container tr:last-child td:last-child {\r\n  border-bottom-right-radius: 10px;\r\n}\r\n\r\n/* Add hover effect for rows */\r\n.table-container tr:hover {\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n  .product-table {\r\n    width: 100%;\r\n    margin: 20px auto;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  th, td {\r\n    padding: 15px;\r\n    text-align: left;\r\n  }\r\n  \r\n  th {\r\n    background-color: #C6CEDC; /* Table header color */\r\n    color: black;\r\n    font-family: 'Urbanist', sans-serif;\r\n    font-weight: 600;\r\n  }\r\n  \r\n  td {\r\n    background-color: #FFFFFF; /* Row color */\r\n    color: black;\r\n    font-family: 'Urbanist', sans-serif;\r\n    font-weight: 400;\r\n  }\r\n  \r\n  tr:nth-child(even) td {\r\n    background-color:  #FFFFFF; /* Alternate row color */\r\n  }\r\n  \r\n  .product-info {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  .product-image {\r\n    width: 50px;\r\n    height: 50px;\r\n    border-radius: 8px;\r\n    margin-right: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;;;;EACO,SAAM;AAAmB;;EAEvB,SAAM;AAAe;;EACnB,SAAM;AAAiB;;EACrB,SAAM;AAAe;;sBAEtBA,mBAAA,CAMQ,gB,aALNA,mBAAA,CAIK,a,aAHHA,mBAAA,CAAgB,YAAZ,SAAO,G,aACXA,mBAAA,CAAc,YAAV,OAAK,G,aACTA,mBAAA,CAAgB,YAAZ,SAAO,E;;;EAKP,SAAM;AAAc;iBAhBxC;;EAiCS,SAAM;AAAc;;EAClB,SAAM;AAAkB;;sBAC3BA,mBAAA,CAAwB,YAApB,iBAAe;AAAA;;EAEZ,SAAM;AAAa;;EACf,OAAI;AAAa;;EAIrB,SAAM;AAAa;;EACf,OAAI;AAAa;;sBAEpBA,mBAAA,CAAwD;IAAhDC,QAAQ,EAAR,EAAQ;IAACC,KAAK,EAAC;KAAG,uBAAqB;AAAA;kBA7C/D;;sBAoDUF,mBAAA,CAAqE;IAA7DG,IAAI,EAAC,QAAQ;IAAC,SAAM;KAAqB,aAAW;AAAA;;uBAnDpEC,mBAAA,CAuDM,OAvDNC,UAuDM,GAtDJC,mBAAA,mBAAsB,EACtBN,mBAAA,CA2BM,OA3BNO,UA2BM,GA1BJP,mBAAA,CAyBM,OAzBNQ,UAyBM,GAxBJR,mBAAA,CAuBM,OAvBNS,UAuBM,GAtBJT,mBAAA,CAqBQ,gBApBNU,UAMQ,EACRV,mBAAA,CAYQ,iB,kBAXNI,mBAAA,CAUKO,SAAA,QAzBnBC,WAAA,CAe6CC,KAAA,CAAAC,QAAQ,EAfrD,UAe0BC,OAAO,EAAEC,KAAK;yBAA1BZ,mBAAA,CAUK;MAVqCa,GAAG,EAAED;IAAK,IAClDhB,mBAAA,CAEK,MAFLkB,UAEK,GADHlB,mBAAA,CAA+B,cAAAmB,gBAAA,CAAtBJ,OAAO,CAACK,IAAI,iB,GAEvBpB,mBAAA,CAA4B,YAAAmB,gBAAA,CAArBJ,OAAO,CAACM,KAAK,kBACpBrB,mBAAA,CAIK,aAHHA,mBAAA,CAES;MAFD,SAAM,eAAe;MAAEsB,OAAK,WAALA,OAAKA,CAAAC,MAAA;QAAA,OAAEC,QAAA,CAAAC,aAAa,CAACV,OAAO,CAACW,EAAE;MAAA;OAAG,OAEjE,iBAvBlBC,UAAA,E;4CAgCIrB,mBAAA,yBAA4B,EAC5BN,mBAAA,CAsBM,OAtBN4B,UAsBM,GAr